package com.index;

public class PolymorphismSample {
	public int add(int x,int y)
	{
		return x+y;
	}
	public int add(int x, int y,int z)
	{
		return x+y+z;
	}
	public double add(double x, double y)
	{
		return x+y;
	}

	public static void main(String[] args) {
		PolymorphismSample sp=new PolymorphismSample();
		
	    int sum1=sp.add(2, 3);
		int sum2=sp.add(2,3,4);
		double sum3=sp.add(9.8, 8.9);
		
		System.out.println("The value of Sum1 is "+sum1);
		System.out.println("The value of Sum2 is "+sum2);
		System.out.println("The value of Sum3 is "+sum3);
		

	}

}
