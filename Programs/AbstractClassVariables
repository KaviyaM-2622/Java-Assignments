package com.index;
abstract class Person
{
	String name;
	Person (String name)
	{
		this.name=name;
	}
abstract void display();
}

public class AbstractClassUsingVariables extends Person {
	AbstractClassUsingVariables(String name)
	{
		super(name);
	}
	void display()
	{
		System.out.println("Name "+name);
	}
	

	public static void main(String[] args) {
		Person p=new AbstractClassUsingVariables("Kaviya");
		p.display();
	

	}

}
