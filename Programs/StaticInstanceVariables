package com.index;

public class Static_Instance_Variables {
	
	static int x=50;
	int y=25;
	
	void show() {
		
		System.out.println("Static Variable x=" +x);
		System.out.println("Instance Variable y=" +y);
		
	}

	public static void main(String[] args) {
		
		Static_Instance_Variables obj1 = new Static_Instance_Variables();
		Static_Instance_Variables obj2 = new Static_Instance_Variables();
		
		obj1.y=20;
		obj2.y=10;
		
		Static_Instance_Variables.x = 100;
		
		obj1.show();
		obj2.show();
		
		
		

	}

}
