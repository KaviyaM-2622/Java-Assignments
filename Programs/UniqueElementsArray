package com.index;

public class UniqueElementsArray {

	public static void main(String[] args) {
		int[]arr= {1,2,2,3,4,4};
		
		for(int i=0;i<arr.length;i++)//picks an element
		{
			boolean isDuplicate=false;
			
			for(int j=0;j<i;j++)//if its element appeared
			{
				if(arr[i]==arr[j])//linear search
				{
					isDuplicate=true;
					break;
				}
			}
			if(!isDuplicate)
			{
				System.out.print(arr[i]+ " ");
			}
		}

	}

}
