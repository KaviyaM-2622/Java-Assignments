package com.index;
//parent class
class EmployeeNew{
	void work()
	{
		System.out.println("Employee is working");
	}
}
//child class
class Developer extends EmployeeNew
{
	void develop()
	{
		System.out.println("Developer develop the product");
	}
}
	//child class 2
	class Tester extends EmployeeNew
	{
		void test()
		{
			System.out.println("Tester tests the product");
		}
	}


public class HierachicalSample {

	public static void main(String[] args) {
		Developer dev=new Developer();
		Tester tes=new Tester();
		
		dev.work();
		dev.develop();
		
		tes.work();
		tes.test();

	}

}
