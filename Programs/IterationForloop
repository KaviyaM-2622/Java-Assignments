package com.index;

public class Iteration_ForLoop {

	public static void main(String[] args) {
		//for (data type variable+0;condition;increment/decrement)
		System.out.println("Printing 1 to 10");
		for (int i=0;i<=10;i++)
		{
			System.out.println(i);		
		}
		System.out.println("Printing 10 to 1");
		for (int i=10;i>=0;i--)
		{
			System.out.println(i);
		}

	}

}
