package com.index;

public final class Lo_Inst_Sta_Variables {
	static int a=30;
	int b=8;
	
	void show() {
		int z=14;
		System.out.println("Static a=" + a);
		System.out.println("Instance b=" + b);
		System.out.println("Local z=" + z);
		
	}

	public static void main(String[] args) {
		 Lo_Inst_Sta_Variables obj = new  Lo_Inst_Sta_Variables();
		 obj.show();

	}

}
