package Assignents;
abstract class Employee
{
	int id;
	String name;
	Employee(int id, String name)
	{
		this.id=id;
		this.name=name;
	}
	abstract void calculateSalary();
}
class FullTimeEmployee extends Employee
{
	FullTimeEmployee(int id, String name)
	{
		super(id,name);
	}
	void calculateSalary()
	{
		System.out.println("Id:" + id);
		System.out.println("Name:"+ name);
		System.out.println("FullTimeEmployee Salary:30000");
	}
}
class PartTimeEmployee extends Employee
{
	PartTimeEmployee(int id, String name)
	{
		super(id,name);
	}
	void calculateSalary()
	{
		System.out.println("Id:" + id);
		System.out.println("Name:"+ name);
		System.out.println("PartTimeEmployee Salary:15000");
	}
}
public class AbstractClassWithVariables {
	public static void main(String[] args) {
		Employee e1=new FullTimeEmployee(111,"Kaviya");
		e1.calculateSalary();
		System.out.println();
		
		Employee e2=new PartTimeEmployee(222,"Meera");
		e2.calculateSalary();
		System.out.println();
		}
	}
