package Assignents;

abstract class Vehicle
{
	abstract void calculateMileage();
	void startEngine()
	{
		System.out.println("Engine started");
		}
}

class Car extends Vehicle
{
	void calculateMileage()
	{
		System.out.println("Car mileage is 30km");
	}
}

class Bike extends Vehicle
{
	void calculateMileage()
	{
		System.out.println("Bike mileage is 25km");
	}
}
public class AbstractClass {
	
	public static void main(String[] args) {
		Vehicle v1=new Car();
		v1.startEngine();
		v1.calculateMileage();
		System.out.println();
		
		Vehicle v2=new Bike();
		v2.startEngine();
		v2.calculateMileage();
		System.out.println();
		}

}
